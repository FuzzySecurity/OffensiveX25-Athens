{
  "name": "starlight",
  "displayName": "Starlight",
  "description": "Nothing passes door and window here save moonlight and starlight",
  "version": "0.0.1",
  "author": "Ruben Boonen",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onStartupFinished"
